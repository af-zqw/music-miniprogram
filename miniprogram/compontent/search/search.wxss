.container{
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  position: relative;
}
.bar{
  flex: 1;
  height: 68rpx;
  font-size: 28rpx;
  border-radius: 50px;
  background-color: #f5f5f5;
  padding-left: 60rpx;
}
.search{
  background: #42cccc;
  color: #fff;
  margin-left: 20rpx;
  font-size: 25rpx;
  border-color:#42cccc;
  letter-spacing:4rpx;
  padding-left: 26rpx;
  padding-right: 26rpx;
}
.search::after{
  border: 0px !important;
}
.find{
  position: absolute;
  top: 50%;
  transform: translate(16rpx,-50%);
  color: #9f9f9f;
}