.index-container{
  width: 100%;
  height: 800rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.icon-wrapper{
  display: flex;
  justify-content: space-around;
}
.img-conent{
  padding: 20rpx 20rpx 10rpx;
  text-align: center;
  overflow: hidden;
  background: #ccc;
  border-radius: 6rpx;
}
.image{
  width: 200rpx;
  height: 200rpx;
  display: block;
}